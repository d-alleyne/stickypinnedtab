{
	"manifest_version": 2,
	"name": "Sticky Pinned Tab",
	"description": "Mimic Google Chrome's Pinned Tab behaviour in Firefox",
	"version": "0.0.1",
	"icons": {
		"64": "icons/icon.png"
	},
	"background": {
		"scripts": [
			"background_script.js"
		]
	},
	"browser_action": {
		"default_icon": {
			"64": "icons/icon.png"
		},
		"default_popup": "browserAction/index.html",
		"default_title": "Sticky Pinned Tab"
	}
}